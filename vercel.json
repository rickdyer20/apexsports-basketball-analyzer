{
  "version": 2,
  "name": "apexsports-basketball-analyzer",
  "redirects": [
    {
      "source": "/(.*)",
      "destination": "https://your-cloud-run-service-url.run.app/$1",
      "permanent": false
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    }
  ]
}
